{
  "name": "@dmaximyuk/signature",
  "version": "1.0.13",
  "description": "Quick signature for Telegram and VKontakte",
  "homepage": "https://github.com/dmaximyuk/signature#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmaximyuk/signature.git"
  },
  "bugs": {
    "url": "https://github.com/dmaximyuk/signature/issues"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun run ./index.ts",
    "build": "rm -rf dist && bun x tsc & bun run build:declaration",
    "build:declaration": "bun x tsc --emitDeclarationOnly --project tsconfig.types.json",
    "test": "bun test"
  },
  "keywords": [
    "vk",
    "vkontakte",
    "tg",
    "telegram",
    "sign",
    "signature",
    "auth",
    "init",
    "start",
    "crypto"
  ],
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.3.0",
    "prettier": "^3.6.2"
  }
}
